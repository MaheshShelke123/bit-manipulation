
//abababac
#include <stdio.h>
int charcmp(char a,char b){
    return a-b;
}
int main()
{   int checkC;
    int pos = 0;
  char string[201] = "xjcuelkxvjjwfqyxcotxmdgmcxdgabababacweivvqjjsyvmzeovbomfabababachosfdfpszmuqcmtgqxwfwjabababaczgauhylcizmmqpevofkhdepbkqmxihszllcjquqakcnesiuxuxcnrqfwiinvjwxcplterwbetvrzotaforwznutolluxknzyotlcwzpwcb";
string[201] ='\0';
for(int i=0;i<200;i++){
   checkC = charcmp(string[i],'c');
   if(i >= 7 && checkC == 0){ 
       // because the length of the substring is 8
       if( charcmp(string[i-1],'a') == 0 && charcmp(string[i-2],'b') == 0 && charcmp(string[i-3],'a') == 0 && charcmp(string[i-4],'b') == 0 && charcmp(string[i-5],'a') == 0 && charcmp(string[i-6],'b') == 0 && charcmp(string[i-7],'a') == 0){
           printf("%c",string[i+3]);
       }
   }
}


    return 0;
}
